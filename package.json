{
  "name": "json-linter-2",
  "version": "1.0.1",
  "description": "JSON linter",
  "contributors": [
    "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
    "Zach Carter <zach@carter.name> (http://zaa.ch)"
  ],
  "bin": {
    "json-lint": "./bin/json-lint.js"
  },
  "keywords": [
    "lint",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medikoo/json-linter.git"
  },
  "dependencies": {
    "es5-ext": "^0.10.46",
    "fs2": "^0.3",
    "log": "^3.2",
    "log-node": "^3.2.1",
    "minimist": "^1.2"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-medikoo": "2",
    "jison": "^0.4.18"
  },
  "eslintConfig": {
    "extends": "medikoo",
    "root": true,
    "env": {
      "node": true
    },
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "max-lines": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "lib/parser.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "node test/index.js"
  },
  "license": "ISC"
}
