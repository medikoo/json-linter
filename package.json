{
  "name": "json-linter",
  "version": "1.0.0",
  "description": "JSON linter",
  "contributors": [
    "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
    "Zach Carter <zach@carter.name> (http://zaa.ch)"
  ],
  "keywords": [
    "lint",
    "json"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/medikoo/json-linter.git"
  },
  "dependencies": {
    "es5-ext": "^0.10.46"
  },
  "devDependencies": {
    "eslint": "^5.11.1",
    "eslint-config-medikoo": "2",
    "jison": "^0.4.18"
  },
  "eslintConfig": {
    "extends": "medikoo",
    "root": true,
    "env": {
      "node": true
    },
    "overrides": [
      {
        "files": [
          "test/**/*.js"
        ],
        "rules": {
          "max-lines": "off"
        }
      }
    ]
  },
  "eslintIgnore": [
    "index.js"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "node test/index.js"
  },
  "license": "ISC"
}
